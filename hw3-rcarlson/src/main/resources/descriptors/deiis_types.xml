<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>deiis_types</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.Annotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>The unique identifier of the CAS processor that added this annotation to the CAS</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence level assigned to the Annotation by the CAS processor that created it.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>elementType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>ngrams</name>
          <description>Each answer has a list of 1-, 2-, and 3-grams (possibly more or less).</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.NGram</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>ngrams</name>
          <description>Each question has a list of 1-, 2-, and 3-grams (possibly more or less).</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.NGram</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.AnswerScore</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.Evaluation</name>
      <description>Represents an evaluation of all answers, calculating the Precision@N value and displaying the sorted answers.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>precisionAtN</name>
          <description>Precision@N, where N is the number of correct answers.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answers</name>
          <description>a sorted list of answer scores, from which we can calculate the Precision@N.</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.AnswerScore</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
